{
  "name": "better-commit",
  "displayName": "Better Commit",
  "description": "VSCode extension that keeps your commit messages consistent",
  "version": "1.0.0",
  "publisher": "ugistelmokaitis",
  "engines": {
    "vscode": "^1.80.0"
  },
  "license": "MIT",
  "homepage": "https://ugistelmokaitis.com",
  "bugs": {
    "url": "https://github.com/ugistelmokaitis/better-commit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ugistelmokaitis/better-commit"
  },
  "icon": "icon.png",
  "keywords": [
    "git",
    "commit",
    "vscode",
    "better commit",
    "ugistelmokaitis"
  ],
  "categories": [
    "Other"
  ],
  "main": "./out/index.js",
  "activationEvents": [
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.betterCommit",
        "title": "Better Commit",
        "category": "Other",
        "icon": "icon.png"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "command": "extension.betterCommit",
          "group": "navigation",
          "when": "scmProvider == git"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.betterCommit",
        "key": "ctrl+alt+enter",
        "mac": "cmd+shift+enter"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}
